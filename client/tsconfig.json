{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "jsx": "React",
    "jsxFactory": "h",
    "newLine": "LF",
    "removeComments": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  }
}
