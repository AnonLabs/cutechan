{
  "compilerOptions": {
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2017.object"],
    "downlevelIteration": true,
    "module": "amd",
    "moduleResolution": "node",
    "jsx": "React",
    "jsxFactory": "h",
    "newLine": "LF",
    "removeComments": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  }
}
